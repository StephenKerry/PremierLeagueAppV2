{{#if teams.teams}}

  <table class="ui fixed striped table">
    <thead>
      <tr>
        <th>Team</th>
        <th>Manager</th>
        <th>Stadium</th>
        <th>City</th>
      </tr>
    </thead>
    <tbody>
      {{#each teams.teams}}
        <tr>
          <td>{{name}}</td>
          <td>{{manager}}</td>
          <td>{{stadium}}</td> 
          <td>{{City}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>

{{else}}
  <h3 class="ui message">
    No teams available!
  </h3>
{{/if}}
